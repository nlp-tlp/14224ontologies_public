###  https://pid.vocab14224.org/TermsAndDefinitions/Fault
voc:Fault rdf:type owl:Class ;
          rdfs:label "Fault"@en ;
          rdfs:seeAlso ""@en ;
          skos:altLabel ""@en ;
          skos:definition "inability to perform as required, due to an internal state"@en ;
          skos:example ""@en ;
