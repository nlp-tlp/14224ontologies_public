###  https://pid.vocab14224.org/TermsAndDefinitions/CriticalFailure
voc:CriticalFailure rdf:type owl:Class ;
                    rdfs:label "Critical failure"@en ;
                    rdfs:seeAlso ""@en ;
                    skos:altLabel ""@en ;
                    skos:definition "failure of an equipment unit that causes an immediate cessation of the ability to perform a required function"@en ;
                    skos:example ""@en ;
                    skos:scopeNote ""@en ;
                    voc:ont00001760 "https://pid.vocab14224.org/TermsAndDefinitions"^^xsd:anyURI ;
                    cmns-av:adaptedFrom ""@en ;
                    cmns-av:directSource "ISO 14224:2023 Terms and Definitions"@en ;
                    cmns-av:explanatoryNote "a critical failure results in an unscheduled repair "@en ,
                                            "includes failures requiring immediate action towards cessation of performing the function, even though actual operation can continue for a short period of time"@en ;
                    cmns-av:usageNote ""@en .


###  https://pid.vocab14224.org/TermsAndDefinitions/DegradedFailure
voc:DegradedFailure rdf:type owl:Class ;
                    rdfs:label "Degraded failure"@en ;
                    rdfs:seeAlso ""@en ;
                    skos:altLabel ""@en ;
                    skos:definition "failure that does not cease the fundamental function(s), but compromises one or several functions"@en ;
                    skos:example ""@en ;
                    skos:scopeNote ""@en ;
                    voc:ont00001760 "https://pid.vocab14224.org/TermsAndDefinitions"^^xsd:anyURI ;
                    cmns-av:adaptedFrom ""@en ;
                    cmns-av:directSource "ISO 14224:2023 Terms and Definitions"@en ;
                    cmns-av:explanatoryNote "an immediate repair can normally be delayed but, in time, such failures can develop into a critical failure if corrective actions are not taken "@en ,
                                            "the failure can be gradual, partial or both"@en ,
                                            "the function can be compromised by any combination of reduced, increased or erratic outputs "@en ;
                    cmns-av:usageNote ""@en .


